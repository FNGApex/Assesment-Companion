{
   "name": "Assessment Companion",
   "description": "Allows Students to take content assesments securely using a browser lockdown",
   "version": "0.1.5",
   "manifest_version": 3,
   "permissions": [
      "tabs"
   ],
   "background": {
      "service_worker": "js/background.js"
   },
   "externally_connectable": {
      "matches": [ "*://*.summitlearning.org/*", "http://localhost/*", "https://*.czedi.com/*" ]
   },
   "icons": {
      "128": "images/icons/icon128.png",
      "16": "images/icons/icon16.png",
      "48": "images/icons/icon48.png"
   },
   "key": ""
}
